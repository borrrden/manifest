<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="couchbase" fetch="https://github.com/couchbase/"/>
  <remote name="couchbasedeps" fetch="https://github.com/couchbasedeps/"/>

  <default remote="couchbase" revision="master"/>

  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- Couchbase-owned primary repository                 -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <project name="libsodium" path="libsodium" remote="couchbasedeps" revision="refs/tags/1.0.18-RELEASE"/>

  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <!-- Build support - build scripts, etc.                -->
  <!-- ++++++++++++++++++++++++++++++++++++++++++++++++++ -->
  <project name="build-tools" path="build-tools"/>
  <project name="build" path="cbbuild">
    <annotation name="VERSION" value="1.0.18" keep="true"/>
  </project>
</manifest>
